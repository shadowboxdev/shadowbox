  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && electron-rebuild install-app-deps",
    "nx": "nx",
    "start": "npm run postinstall.web && ng serve devops",
    "build": "npm run postinstall.web && ng build devops",
    "test": "npm run postinstall.web && ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "npm run postinstall.web && ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "build:pwa": "webpack swtemplate.ts --config ./apps/devops/src/workbox/webpack.config.js",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall.electron": "node tools/electron/postinstall",
    "postinstall.web": "node tools/web/postinstall",
    "build.electron.ui": "npm run prepare.electron.ui && ng build electron-ui --prod --base-href ./",
    "build.electron.ui.local": "npm run build.electron.ui && electron dist/apps/electron-ui",
    "build.electron.ui.linux": "npm run build.electron.ui && cd dist/apps/electron-ui && npx electron-builder build --linux",
    "build.electron.ui.windows": "npm run build.electron.ui && cd dist/apps/electron-ui && npx electron-builder build --windows",
    "build.electron.ui.mac": "npm run build.electron.ui && cd dist/apps/electron-ui && npx electron-builder build --mac",
    "prepare.electron.ui": "npm run postinstall.electron && tsc -p apps/electron-ui/tsconfig.json",
    "serve.electron.ui.target": "nx serve electron-ui",
    "serve.electron.ui": "wait-on http-get://localhost:4200/ && electron apps/electron-ui/src --serve",
    "start.electron.ui": "npm run prepare.electron.ui && npm-run-all -p serve.electron.ui.target serve.electron.ui",
    "start.devops": "npm run postinstall.web && nx serve devops"
  },
